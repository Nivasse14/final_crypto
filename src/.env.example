# Variables d'environnement pour le Copy Trading ETL

# Base de données PostgreSQL
DATABASE_URL=postgresql://username:password@localhost:5432/copy_trading

# API Configuration
PORT=3000

# Cielo API (pour récupération des données wallets)
CIELO_API_URL=http://localhost:54321/functions/v1/cielo-api

# Prix APIs
DEXSCREENER_API_URL=https://api.dexscreener.com/latest/dex
JUPITER_API_URL=https://price.jup.ag/v4

# Timeouts et limites (optionnel)
PRICE_FETCH_TIMEOUT=10000
REQUEST_BATCH_SIZE=20
CACHE_TTL_SECONDS=60

# Logging
LOG_LEVEL=info
