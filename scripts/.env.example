# Configuration pour les scripts de collecte de données

# Supabase (obligatoire pour l'upload des données)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_key_here

# APIs externes (optionnel selon les scripts utilisés)
COINGECKO_API_KEY=your_coingecko_api_key
GMGN_API_KEY=your_gmgn_key_if_needed
CIELO_API_URL=your_cielo_api_url_if_available
CIELO_API_KEY=your_cielo_key_if_needed

# Configuration Puppeteer (pour dune-scraper.js)
PUPPETEER_HEADLESS=true
PUPPETEER_TIMEOUT=60000
